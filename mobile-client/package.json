{
  "name": "budget-app-mobile",
  "version": "1.0.0",
  "main": "App.tsx",
  "scripts": {
    "start": "EXPO_NO_DEPENDENCY_VALIDATION=1 node node_modules/expo/bin/cli start",
    "start:clear": "EXPO_NO_DEPENDENCY_VALIDATION=1 node node_modules/expo/bin/cli start --clear",
    "start:mcp": "EXPO_UNSTABLE_MCP_SERVER=1 EXPO_NO_DEPENDENCY_VALIDATION=1 node node_modules/expo/bin/cli start",
    "start:mcp:clear": "EXPO_UNSTABLE_MCP_SERVER=1 EXPO_NO_DEPENDENCY_VALIDATION=1 node node_modules/expo/bin/cli start --clear",
    "typecheck": "node node_modules/typescript/bin/tsc --noEmit",
    "doctor": "npx expo-doctor",
    "android": "EXPO_NO_DEPENDENCY_VALIDATION=1 node node_modules/expo/bin/cli start --android",
    "ios": "node node_modules/expo/bin/cli run:ios",
    "prebuild": "node node_modules/expo/bin/cli prebuild --clean",
    "build:ios:beta": "eas build --platform ios --profile beta",
    "submit:ios:beta": "eas submit --platform ios --profile beta",
    "web": "EXPO_NO_DEPENDENCY_VALIDATION=1 node node_modules/expo/bin/cli start --web",
    "postinstall": "node scripts/fix-expo-metro-shims.cjs"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.4.2",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.6.0",
    "@react-navigation/bottom-tabs": "^7.14.0",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/native-stack": "^7.13.0",
    "@reduxjs/toolkit": "^2.11.2",
    "expo": "~55.0.0",
    "expo-blur": "~55.0.8",
    "expo-constants": "~55.0.7",
    "expo-device": "~55.0.9",
    "expo-font": "~55.0.4",
    "expo-image-picker": "~55.0.10",
    "expo-notifications": "~55.0.10",
    "expo-secure-store": "~55.0.8",
    "expo-splash-screen": "~55.0.10",
    "expo-status-bar": "~55.0.4",
    "expo-task-manager": "~55.0.9",
    "expo-updates": "~55.0.12",
    "lucide-react-native": "^0.575.0",
    "react": "19.2.0",
    "react-native": "0.83.2",
    "react-native-gifted-charts": "^1.4.74",
    "react-native-linear-gradient": "^2.8.3",
