{
  "cli": {
    "version": ">= 12.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "beta": {
      "distribution": "store",
      "channel": "beta",
      "autoIncrement": "buildNumber"
    },
    "production": {
      "distribution": "store",
      "channel": "production",
      "autoIncrement": "buildNumber"
    }
  },
  "submit": {
    "beta": {
      "ios": {}
    },
    "production": {
      "ios": {}
    }
  }
}
