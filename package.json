{
  "name": "budget-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run prisma:generate && prisma migrate deploy && NEXT_DISABLE_TURBOPACK=1 next dev -p 5537",
    "dev:turbo": "npm run prisma:generate && prisma migrate deploy && next dev -p 5537",
    "dev:clean": "rm -rf .next && npm run dev",
    "build": "prisma generate && prisma migrate deploy && next build",
    "start": "next start",
    "lint": "eslint",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "db:seed": "tsx prisma/seed.ts",
    "postinstall": "prisma generate",
    "test:api": "tsx scripts/test-api-integration.ts",
    "sync:categories": "tsx scripts/sync-default-categories.ts",
    "read:xlsx": "tsx scripts/read-xlsx.ts --file '5 YEAR FINANCIAL FORECAST.xlsx'",
    "formulas": "tsx scripts/extract-formulas.ts --file '5 YEAR FINANCIAL FORECAST.xlsx' --sheet 'PERSONAL BUDGET 2026-2027'",
    "budget:test": "tsx scripts/test-budget-engine.ts",
    "export:xlsx:json": "tsx scripts/export-xlsx.ts --file '5 YEAR FINANCIAL FORECAST.xlsx' --format json --out data/forecast.json",
    "export:xlsx:csv": "tsx scripts/export-xlsx.ts --file '5 YEAR FINANCIAL FORECAST.xlsx' --format csv --out data/forecast.csv",
    "migrate:debt-ids": "tsx scripts/migrate-debt-ids.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@prisma/client": "^6.3.1",
    "@reduxjs/toolkit": "^2.5.1",
    "chart.js": "^4.5.1",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "next-auth": "^4.24.13",
    "react": "19.2.3",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "19.2.3",
    "react-redux": "^9.2.0",
    "web-push": "^3.6.7",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@ducanh2912/next-pwa": "^10.2.9",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-redux": "^7.1.34",
    "@types/web-push": "^3.6.4",
    "@types/xlsx": "^0.0.36",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "prisma": "^6.3.1",
    "tailwindcss": "^4",
    "tsx": "^4.0.0",
    "typescript": "^5"
  }
}
