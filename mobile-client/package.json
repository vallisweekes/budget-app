  "devDependencies": {
    "@expo/metro": "~54.2.0",
    "@types/react": "~19.2.10",
    "@types/react-redux": "^7.1.34",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "~55.0.8",
    "expo-mcp": "~0.2.1",
    "expo-module-scripts": "^5.0.8",
    "react-native-svg-transformer": "^1.5.3",
    "typescript": "~5.9.2"
